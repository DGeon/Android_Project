<?xml version="1.0" encoding="utf-8"?>

<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.firstcalc.amiCalc">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE"/>

    <application
        android:fullBackupContent="false"
        android:allowBackup="false"
        android:usesCleartextTraffic="true"
        android:icon="@mipmap/ic_appicon"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_appicon"
        android:supportsRtl="true"
        android:theme="@style/Theme.amiCalcApp">
        <activity android:name="com.firstcalc.amiCalc.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.firstcalc.amiCalc.Join"/>
        <activity android:name="com.firstcalc.amiCalc.PrevAmi"/>
        <activity android:name="com.firstcalc.amiCalc.Ami"/>
        <activity android:name="com.firstcalc.amiCalc.Bus"/>
        <activity android:name="com.firstcalc.amiCalc.MyPage"/>
        <activity android:name="com.firstcalc.amiCalc.Work"/>
        <activity android:name="com.firstcalc.amiCalc.AmiUpdate"/>
        <activity android:name="com.firstcalc.amiCalc.PayCheck"/>
        <activity android:name="com.firstcalc.amiCalc.OpenInfo"/>
    </application>

</manifest>